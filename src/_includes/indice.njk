<!-- Index section for medium & large medium -->
<h1 class="w3-center w3-dark-gray">El Ã­ndice</h1>
<div class="w3-wide">
	<div class="w3-bar">
		{% for item in sections %}
		<a class="w3-btn w3-left-align w3-mobile" style="width:23.9%" href="#{{ item | Ascii }}">{{ item }}</a>
		{% endfor %}
		<a class="w3-btn w3-left-align w3-mobile" style="width:23.9%" href="utillaje.html">Utillaje</a>
	</div>
</div>

<!-- Navigation popup for Secciones -->
<div id="popup_sections" class="w3-modal" style="">
  <div class="w3-modal-content" style="width:300px;top:8px">
		<div class="w3-bar-block w3-wide">
			<button class="w3-btn w3-right" onclick="document.getElementById('popup_sections').style.display='none'">&times</button>
			{% for item in sections %}
			<a class="w3-btn w3-bar-item w3-mobile" href="#{{ item | Ascii }}" onclick="document.getElementById('popup_sections').style.display='none'">{{ item }}</a>
			{% endfor %}
			<a class="w3-btn w3-bar-item w3-mobile" href="utillaje.html" onclick="document.getElementById('popup_sections').style.display='none'">Utillaje</a>
		</div>
  </div>
</div>



